Algorithme:"palindrom";
declaration:
    var:
        mot[]:string;
        i: entier;

debut:

    for (i=0, i<=mot.length-1-i){
        if (i=mot.length-1-i et mot[i]=mot[mot.length-1-i])
            console.log(mot" est un palindrome");
            else if (mot[i]!=mot[mot.length-1-i]);
            console.log(mot" n'est pas un palindrome");
            end else if
        end if
        
    end for
        
    }
    